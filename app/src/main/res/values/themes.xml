        <item name="colorPrimaryVariant">#AB192D</item>
        <item name="colorOnPrimary">#777676</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">#b2263c</item>
        <item name="colorSecondaryVariant">#AB192D</item>
